{
  "name": "elysia-kick-start",
  "version": "1.0.50",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"bun:dev:watch\" \"bun:db:studio\"",
    "dev:watch": "bun --bun run --watch src/index.ts",
    "start": "bun --bun run src/index.ts",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "prepare": "husky"
  },
  "dependencies": {
    "@elysiajs/bearer": "^1.2.0",
    "@elysiajs/cors": "^1.2.0",
    "@elysiajs/jwt": "^1.2.0",
    "@elysiajs/server-timing": "^1.2.0",
    "@elysiajs/swagger": "^1.2.0",
    "@faker-js/faker": "^9.5.1",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.39.3",
    "drizzle-typebox": "^0.3.0",
    "elysia": "latest",
    "ioredis": "^5.6.0",
    "logixlysia": "^4.1.1",
    "mysql2": "^3.12.0"
  },
  "devDependencies": {
    "@types/bun": "^1.2.2",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "bun-types": "latest",
    "concurrently": "^9.1.2",
    "drizzle-kit": "^0.30.4",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "prettier": "^3.4.2",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3"
  },
  "override": {
    "@sinclair/typebox": "^0.34.15"
  },
  "module": "src/index.js",
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
